FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (30.0 0 0);

boundaryField
{
    inlet
    {
        type            freestreamVelocity;
        freestreamValue uniform (30.0 0 0);
    }

    outlet
    {
        type            freestreamVelocity;
        freestreamValue uniform (30.0 0 0);
    }

    domain
    {
        type            freestreamVelocity;
        freestreamValue uniform (30.0 0 0);
    }

    wall
    {
        type            noSlip;
    }
}

