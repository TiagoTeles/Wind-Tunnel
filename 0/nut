FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.5e-06;

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform 1.5e-06;
    }

    outlet
    {
        type            freestream;
        freestreamValue uniform 1.5e-06;
    }

    domain
    {
        type            freestream;
        freestreamValue uniform 1.5e-06;
    }

    wall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}

