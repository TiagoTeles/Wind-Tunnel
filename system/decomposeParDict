FoamFile
{
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  6;

method hierarchical;

hierarchicalCoeffs
{
    n       (3 2 1);
    order   xyz;
    delta   0.001;
}

